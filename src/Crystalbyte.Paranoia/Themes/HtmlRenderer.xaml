<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:ui="clr-namespace:Crystalbyte.Paranoia.UI">
    <Style TargetType="{x:Type ui:HtmlRenderer}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ui:HtmlRenderer">
                    <FlowDocumentReader>
                        <FlowDocument x:Name="PART_XamlHost"
                                      Foreground="WhiteSmoke"
                                      PagePadding="0" />
                    </FlowDocumentReader>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>

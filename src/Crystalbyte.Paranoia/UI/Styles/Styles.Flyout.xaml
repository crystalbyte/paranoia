<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--  Flyout  -->
    <SolidColorBrush x:Key="FlyoutBackgroundBrushKey" Color="#353539" />
    <SolidColorBrush x:Key="FlyoutForegroundBrushKey" Color="WhiteSmoke" />
    <SolidColorBrush x:Key="FlyoutOverlayBrushKey" Color="Black" />
    <SolidColorBrush x:Key="FlyoutButtonBackgroundBrushKey" Color="#2F2F30" />
    <SolidColorBrush x:Key="FlyoutButtonBorderBrushKey" Color="#555556" />
    <SolidColorBrush x:Key="FlyoutRadioButtonBackgroundBrushKey" Color="#2F2F32" />
    <SolidColorBrush x:Key="FlyoutRadioButtonBorderBrushKey" Color="#555556" />
    <SolidColorBrush x:Key="FlyoutComboBoxBackgroundBrushKey" Color="#2F2F32" />
    <SolidColorBrush x:Key="FlyoutComboBoxBorderBrushKey" Color="#555556" />
    <SolidColorBrush x:Key="FlyoutTextBoxBackgroundBrushKey" Color="#2F2F32" />
    <SolidColorBrush x:Key="FlyoutTextBoxBorderBrushKey" Color="#555556" />
    <system:Double x:Key="FlyoutOverlayOpacityKey">.5</system:Double>

</ResourceDictionary>
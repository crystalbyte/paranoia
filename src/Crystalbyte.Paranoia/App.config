<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <nlog>
    <targets async="true">
      <target name="f1" type="File" fileName="paranoia.log" layout="${longdate} ${callsite} ${level} ${message}" />
    </targets>
    <rules>
      <logger name="Crystalbyte.Paranoia.Log" levels="Debug,Error" writeTo="f1" />
    </rules>
  </nlog>
</configuration>
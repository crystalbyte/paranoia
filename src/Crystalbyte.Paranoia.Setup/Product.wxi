  <!-- Keys for ProgId Paranoia.Application -->
  <?define VerIndAppProgId=$(var.ProductName).Application?>
  <?define AppProgId=$(var.VerIndAppProgId).$(var.MajorVersion)?>
  <?define AppClassId=AA1CC4EE-2EB6-4521-940B-5B4C56C46CB0?>
  
</Include>

<?xml version="1.0" encoding="utf-8"?>
<Include>
  <!-- Package Information-->
  <?define ProductName="Paranoia" ?>
  <?define Manufacturer="Crystalbyte" ?>
  <?define FriendlyName="$(var.ProductName)" ?>
